        <th>
            @Html.DisplayNameFor(model => model.Model_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Popularity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddedDate)
        </th>
