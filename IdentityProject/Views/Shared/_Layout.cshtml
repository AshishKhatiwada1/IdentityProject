<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="jumbotron">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                </div>

                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("Manufacturers", "Index", "Manufacturers")</li>
                        <li>@Html.ActionLink("Vehicle Types","Index","VehicleTypes")</li>
                        <li>@Html.ActionLink("Vehicle Models", "Index", "VehicleModels")</li>
                        <li>@Html.ActionLink("Variants", "Index", "Variants")</li>
                        <li>@Html.ActionLink("Engine Emission", "Index", "EngineEmissions")</li>
                        <li>@Html.ActionLink("User Rating", "Index", "UserRatings")</li>
                        <li>@Html.ActionLink("Vehicle Medias", "Index", "VehicleMedias")</li>
                        <li>@Html.ActionLink("Add Vehicle","Index","AddVehicleViewModels")</li>

                        @*<li>@Html.ActionLink("Variants", "Index", "Variants")</li>
                            <li>@Html.ActionLink("About", "About", "Home")</li>
                            <li>@Html.ActionLink("Colors", "Index", "Colors")</li>
                            <li>@Html.ActionLink("EngineEmission", "Index", "EngineEmissions")</li>
                            <li>@Html.ActionLink("UserRatings", "Index", "UserRatings")</li>


                            <li>@Html.ActionLink("Cities", "Index", "Cities")</li>
                            <li>@Html.ActionLink("Continents", "Index", "Continents")</li>
                            <li>@Html.ActionLink("Countries", "Index", "Countries")</li>
                            <li>@Html.ActionLink("ShippingAddresses", "Index", "ShippingAddresses")</li>
                            <li>@Html.ActionLink("States", "Index", "States")</li>
                            <li>@Html.ActionLink("Streets", "Index", "Streets")</li>
                            <li>@Html.ActionLink("User Addresses", "Index", "UserAddresses")</li>*@
                    </ul>
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div class="jumbotron">
            @RenderBody()
        </div>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
